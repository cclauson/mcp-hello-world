# Auth provider: 'auth0' or 'entra'
AUTH_PROVIDER=auth0

# Auth0 (when AUTH_PROVIDER=auth0)
AUTH0_DOMAIN=your-tenant.us.auth0.com
AUTH0_AUDIENCE=https://mcp-hello-world

# Entra External ID (when AUTH_PROVIDER=entra)
ENTRA_TENANT_ID=your-tenant-id
ENTRA_TENANT_NAME=your-tenant-name
ENTRA_CLIENT_ID=your-client-id

PORT=3000
